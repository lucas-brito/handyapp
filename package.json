{
  "name": "handyapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint app/*.js",
    "lint:tests": "eslint test/**/*.js",
    "translate": "node i18n.extract.js",
    "build:android": "cd android && ./gradlew assembleRelease && open app/build/outputs/apk",
    "detox:build:android": "detox build --configuration android.debug",
    "detox:test:android": "detox test --configuration android.debug",
    "detox:build:ios": "detox build --configuration ios.debug",
    "detox:test:ios": "detox test --configuration ios.debug"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.11.0",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "5.5.1",
    "@react-navigation/stack": "5.5.1",
    "lodash": "^4.17.19",
    "mobx": "5.15.4",
    "mobx-react": "6.2.2",
    "moment": "2.29.2",
    "react": "16.11.0",
    "react-native": "0.62.2",
    "react-native-drawer": "^2.5.1",
    "react-native-gesture-handler": "1.6.1",
    "react-native-localize": "1.3.0",
    "react-native-reanimated": "1.9.0",
    "react-native-safe-area-context": "3.0.5",
    "react-native-screens": "2.8.0",
    "react-native-tiny-toast": "^1.0.7",
    "react-native-vector-icons": "6.6.0",
    "react-navigation-props-mapper": "1.0.4",
    "sprintf-js": "1.1.2"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-decorators": "7.10.3",
    "@babel/plugin-proposal-optional-chaining": "7.10.3",
    "@babel/preset-flow": "7.10.1",
    "@babel/runtime": "7.10.2",
    "@react-native-community/eslint-config": "2.0.0",
    "babel-jest": "26.0.1",
    "detox": "16.8.2",
    "eslint": "7.2.0",
    "i18n-extract": "0.6.3",
    "jest": "26.0.1",
    "jest-circus": "26.0.1",
    "metro-react-native-babel-preset": "0.59.0",
    "react-test-renderer": "16.11.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
